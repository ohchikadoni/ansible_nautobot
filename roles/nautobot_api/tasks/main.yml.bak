- name: Ensure Nautobot API is reachable
  uri:
    url: "{{ nautobot_api_url }}"
    method: GET
    headers:
      Authorization: "Token {{ nautobot_api_key }}"
      Content-Type: "application/json"
    return_content: yes
  register: nautobot_response
  failed_when: nautobot_response.status != 200

- debug:
    var: nautobot_response.json
